{
  "kind": "collectionType",
  "collectionName": "hotels",
  "info": {
    "singularName": "hotel",
    "pluralName": "hotels",
    "displayName": "hotel",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "HoteIImage": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "HotelTimings": {
      "type": "component",
      "repeatable": true,
      "component": "timing.hotel-timings"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::name.name",
      "mappedBy": "hotels"
    },
    "avgprice": {
      "type": "decimal"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "hotel"
    }
  }
}
